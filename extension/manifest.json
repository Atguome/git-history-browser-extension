{
  "manifest_version": 2,
  "name": "Github History Browser Extension",
  "author": "Luis Reinoso",
  "description": "Agrega un bot√≥n a github para ver el historial del archivo / Add a button to github to see the file history",
  "homepage_url": "https://github.com/LuisReinoso/github-history-browser-extension",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://github.com/*/*/blob/*"],
      "js": ["main.js"]
    }
  ],
  "icons": {
    "16": "icons/github-history-16x16.png",
    "48": "icons/github-history-48x48.png",
    "128": "icons/github-history-128x128.png",
    "500": "icons/github-history-500x500.png"
  }
}
